{
  "name": "i18n-sheets",
  "version": "1.1.2",
  "type": "module",
  "main": "dist/index.mjs",
  "bin": {
    "i18n-sheets": "bin/i18n-sheets"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --target node20 --dts",
    "prepare": "npm run build",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "google-sheets",
    "translation",
    "cli",
    "typescript",
    "sync",
    "localization"
  ],
  "author": "2ast",
  "license": "MIT",
  "description": "A powerful CLI tool for synchronizing internationalization (i18n) resources between Google Sheets and your TypeScript project",
  "repository": {
    "type": "git",
    "url": "https://github.com/DongGukMon/i18n-sheets.git"
  },
  "homepage": "https://github.com/DongGukMon/i18n-sheets#readme",
  "bugs": {
    "url": "https://github.com/DongGukMon/i18n-sheets/issues"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  }
}
